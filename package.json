{
  "name": "novel-reader",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "electron-rebuild sqlite3",
    "dev:app": "ts-node ./scripts/server.ts",
    "dev:web-app": "cross-env WEB_BACKEND_HOST=http://127.0.0.1:10009 DEVELOPMENT_PORT=10007 TARGET_PLATFORM=web npm run dev:app",
    "dev:web-server": "cross-env PLATFORMS_SERVICE_PROXY_ENABLE=true WEB_BACKEND_HOST=http://127.0.0.1:10009 ts-node --project ./src/tsconfig-platforms.json -r tsconfig-paths/register ./src/platforms/web/server.ts",
    "build:platforms": "tsc -p ./src/tsconfig-platforms.json --outDir dist/host && tscpaths -p src/tsconfig-platforms --src src -o dist/host",
    "build:web-app": "ts-node ./scripts/build.ts --target=web --out-dir=./dist/host/platforms/web/app",
    "build:electron-app": "cross-env TARGET_PLATFORM=electron ts-node ./scripts/build.ts --target=electron --out-dir=./dist/host/platforms/electron/app",
    "build:electron-distribution": "ts-node ./scripts/electron/build.ts",
    "dev:elecron-app": "cross-env DEVELOPMENT_PORT=10012 TARGET_PLATFORM=electron npm run dev:app",
    "dev:electon-main": "ts-node ./scripts/electron/dev.ts",
    "dev:electon-main-auto": "ts-node ./scripts/electron/dev.ts --auto-reload=true"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@vue/composition-api": "^0.3.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "events": "^3.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "minio": "^7.0.12",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "translation.js": "^0.7.12",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/chokidar": "^2.1.3",
    "@types/electron-packager": "^14.0.0",
    "@types/events": "^3.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/md5": "^2.1.33",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^12.7.4",
    "@types/parcel-bundler": "^1.12.1",
    "@types/uuid": "^3.4.5",
    "@vue/component-compiler-utils": "^3.0.0",
    "chokidar": "^3.3.0",
    "cross-env": "^6.0.0",
    "electron": "^6.0.12",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.3",
    "tree-kill": "^1.2.1",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "tsconfig-paths": "^3.8.0",
    "tscpaths": "0.0.9",
    "typescript": "^3.6.2",
    "uikit": "^3.2.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "alias": {
    "shared": "./src/shared",
    "app": "./src/app",
    "platforms": "./src/platforms",
    "uikit-util": "./node_modules/uikit/src/js/util"
  }
}
