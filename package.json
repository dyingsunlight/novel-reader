{
  "name": "novel-reader",
  "version": "1.0.0",
  "scripts": {
    "web-dev": "cross-env WEB_BACKEND_HOST=http://127.0.0.1:10009 ts-node ./scripts/server.ts",
    "web-backend-server": "cross-env WEB_BACKEND_HOST=http://127.0.0.1:10009 ts-node --project ./src/tsconfig-platforms.json -r tsconfig-paths/register ./src/platforms/web/server.ts",
    "build:web-server": "tsc -p ./src/tsconfig-platforms.json --outDir dist && tscpaths -p src/tsconfig-platforms --src src -o dist",
    "build:web-app": "parcel build src/app/pages/**/*.html -d dist/platforms/web/app --public-url=./ --no-source-maps",
    "build:web": "npm run build:web-server && npm run build:web-app"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/events": "^3.0.0",
    "@types/md5": "^2.1.33",
    "@vue/composition-api": "^0.3.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "events": "^3.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "md5": "^2.2.1",
    "minio": "^7.0.12",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "translation.js": "^0.7.12",
    "typeorm": "^0.2.20",
    "uikit": "^3.2.1",
    "uuid": "^3.3.3",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^12.7.4",
    "@types/uuid": "^3.4.5",
    "@vue/component-compiler-utils": "^3.0.0",
    "cross-env": "^6.0.0",
    "electron": "^6.0.12",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.3",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "tsconfig-paths": "^3.8.0",
    "tscpaths": "0.0.9",
    "typescript": "^3.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "alias": {
    "shared": "./src/shared",
    "app": "./src/app",
    "platforms": "./src/platforms"
  }
}
